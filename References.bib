	number = {1},
	journaltitle = {J Gynecol Oncol},
	author = {Yokoyama, Yoshihito and Matsushita, Yoko and Shigeto, Tatsuhiko and Futagami, Masayuki and Mizunuma, Hideki},
	urldate = {2014-12-29},
